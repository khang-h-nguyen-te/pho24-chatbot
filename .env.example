# OpenAI API key for the language model and embeddings
OPENAI_API_KEY=your_openai_api_key_here

# LLM model to use - using GPT-4o is recommended
LLM_MODEL=gpt-4o

# Embedding model to use
EMBEDDING_MODEL=text-embedding-3-small

# Supabase credentials for vector store
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# Application settings
DEBUG=0  # Set to 1 to enable debug mode

# Memory token limit for conversation history
MEMORY_TOKEN_LIMIT=10000
